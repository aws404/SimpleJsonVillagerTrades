{
  "schemaVersion": 1,
  "id": "sjvt",
  "version": "${version}",

  "name": "SimpleJsonVillagerTrades",
  "description": "As simple as it sounds, a small jij-able library to modify/add/remove Villager trades via datapacks.",
  "authors": [
    "aws404"
  ],
  "contact": {
    "sources": "https://github.com/aws404/SimpleJsonVillagerTrades",
    "issues": "https://github.com/aws404/SimpleJsonVillagerTrades/issues"
  },

  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.github.aws404.sjvt.SimpleJsonVillagerTradesMod"
    ],
    "fabric-datagen": [
      "com.github.aws404.sjvt.datagen.SJVTDataGenerator"
    ]
  },
  "accessWidener": "sjvt.accesswidener",
  "mixins": [
    "sjvt.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.12.12",
    "fabric": ">=0.80.3",
    "minecraft": "1.20.x",
    "java": ">=17"
  }
}
